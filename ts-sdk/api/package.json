{
  "name": "@crypticdot/defituna-api",
  "version": "3.0.3",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:clean && pnpm build:orval && pnpm build:dist",
    "build:clean": "test -d src/gen && find src/gen -type f -name *.ts -delete || true",
    "build:orval": "pnpm orval",
    "build:dist": "tsup src/index.ts --format cjs,esm --dts",
    "eslint": "eslint \"**/*.ts\"",
    "eslint:fix": "eslint \"**/*.ts\" --fix",
    "type-check": "tsc -p .",
    "lint": "pnpm eslint && pnpm type-check",
    "test": "vitest run --disable-console-intercept",
    "test:start-server": "node scripts/startTestsServer.mjs",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@crypticdot/defituna-client": "^3.0.2",
    "@crypticdot/eslint-config": "^1.0.3",
    "@crypticdot/fusionamm-client": "^1.0.62",
    "@crypticdot/fusionamm-core": "^1.0.62",
    "@crypticdot/prettier-config": "^1.0.0",
    "@crypticdot/typescript-config": "^1.0.0",
    "@solana/kit": "^2.1.0",
    "@types/node": "^22.13.14",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eventsource": "^3.0.6",
    "express": "^4.21.2",
    "openapi3-ts": "^4.5.0",
    "orval": "^7.17.0",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "camelcase-keys": "^10.0.1"
  }
}
