{
  "name": "@crypticdot/defituna-api",
  "version": "4.0.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate": "pnpm openapi-ts",
    "build": "pnpm build:dist",
    "openapi-ts": "node scripts/camelize-openapi.mjs && openapi-ts && node scripts/postprocess-openapi-ts.mjs",
    "build:dist": "tsup src/index.ts --format cjs,esm --dts",
    "eslint": "eslint \"**/*.ts\"",
    "eslint:fix": "eslint \"**/*.ts\" --fix",
    "type-check": "tsc -p .",
    "lint": "pnpm eslint && pnpm type-check",
    "test": "vitest run --disable-console-intercept",
    "test:start-server": "node scripts/startTestsServer.mjs",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@crypticdot/defituna-client": "^3.0.2",
    "@crypticdot/eslint-config": "^1.0.3",
    "@crypticdot/fusionamm-client": "^1.0.62",
    "@crypticdot/fusionamm-core": "^1.0.62",
    "@crypticdot/prettier-config": "^1.0.0",
    "@crypticdot/typescript-config": "^1.0.0",
    "@hey-api/openapi-ts": "0.90.4",
    "@solana/kit": "^2.1.0",
    "@types/node": "^22.13.14",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "express": "^4.21.2",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9",
    "yaml": "^2.8.2",
    "zod": "4.4.0-canary.20251231T055727"
  }
}
